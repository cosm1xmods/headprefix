{
  "schemaVersion": 1,
  "id": "headprefix",
  "version": "${version}",

  "name": "HeadPrefix",
  "description": "Displays configurable prefix above your head.",
  "authors": [
    "Cosm1x"
  ],
  "contact": {
    "sources": "https://github.com/x1msoc/headprefix"
  },

  "license": "CC0-1.0",
  "icon": "assets/headprefix/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "me.cosm1x.headprefix.HeadPrefixMod"
    ],
    "client": [
      "me.cosm1x.headprefix.HeadPrefixModClient"
    ],
    "modmenu": [
      "me.cosm1x.headprefix.integration.ModMenuIntegration"
    ]
  },
  "mixins": [
    "headprefix.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "modemenu": "*"
  }
}
